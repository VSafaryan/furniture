                                <th>
                                    <% i.title.forEach(j => {%>
                                    <p><%= j %></p>
                                    <hr>
                                    <%}) %>
                                </th>
                                <td data-column="Job Title">
                                    <% i.quantity.forEach(k => {%>
                                    <p><%= k %> &nbsp; штук</p>
                                    <hr>
                                    <%}) %>
                                </td>
                                <!-- <td data-column="Twitter">в ожидании</td> -->
                                <td data-column="Twitter"><%= i.totalValue %></td>
                                <td data-column="Twitter"><span class="date"><small><%= i.now %></small></span></td>
                                <!-- <td data-column="Twitter"><a href="" class="primary-button-cub button-xs">Подробнее</a>
                                                    </td> -->
                                <td><%= i.status %></td>
                            </tr>
                            <%} %>
                            <%}) %>
                        </tbody>
                    </table>
                </div>
            </div>
            <%} %>



            <div class="col-lg-4 col-md-5 col-12">
                <div class="register-box-menu">
                    <ul>


                        <% if(isAuth) {%>
                     
                             <%} else {%>
                                 
                                <li><a href="/login"> <%= __('info.login') %> </a></li>
                                <li><a href="/register"> <%= __('info.register') %> </a></li>

                              <%} %>

                        <li><a href="/change-password"> <%= __('info.forgot') %> </a></li>
                        <li><a href="/personal"> <%= __('info.myInfo') %> </a></li>
                        <li class="register-box-menu-active"><a href="/orders"> <%= __('info.orders') %> </a></li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

<!-- Register-box End -->

<%- include('layouts/footer') %>