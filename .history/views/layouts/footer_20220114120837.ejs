                                <!-- <li><a href="#"><i class="fa fa-youtube"></i></a></li> -->
                    </ul>
                </div>

            </div>
            <!-- Column 3 End -->

            <!-- Column 4 Start -->
            <div class="col-md-2 col-sm-6 col-12">
                <div class="footer-tags mt-25">
                    <a href="/condition">Պայմաններ</a>
                </div>
            </div>
            <!-- Column 4 End -->
        </div>
        <!-- Footer Bar Start -->
        <div class="footer-bar">
            <p><a href="http://armcoding.com/" target="_blank"><span class="primary-color"> ArmCoding</span></a> © 2022
                All Rights Reserved.</p>
        </div>
        <!-- Footer Bar End -->
