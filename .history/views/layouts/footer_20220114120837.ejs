<footer>
    <div class="container">
        <div class="row">
            <!-- Column 1 Start -->
            <div class="col-md-4 col-sm-6 col-12">
                <h3>Կապ</h3>
                <div class="phone-footer mt-25">
                    <% if(contact.length) {%>
                        <ul>
                            <li> <i class="fa fa-phone"></i>
