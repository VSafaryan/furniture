                    <ul>
                        <% if(contact.length) {%>
                            <li><a href="http://<%= contact[0].facebook %>" target="_blank"><i
                                        class="fa fa-facebook-square"></i></a></li>
                            <li><a href="http://<%= contact[0].instagram %>" target="_blank"><i
                                        class="fa fa-instagram"></i></a></li>
                            <%} %>
                                <li><a href="#"><i class="icon-feather-mail"></i></a></li>
