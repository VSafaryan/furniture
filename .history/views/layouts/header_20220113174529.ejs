                    <input type="password" name="password" placeholder="<%= __('info.password') %>">
                    <div class="forgot-reg">
                        <a href="/change-password">
                            <%= __('info.forgot') %>
                        </a><br>
                        <a href="/register">
                            <%= __('info.register') %>
                        </a>
                    </div>
                    <button class="submit-button" type="submit">
